 return {\r\n      ...state,\r\n      slideIndex: (state.slideIndex + 1) % slides.length\r\n    };\r\n  }\r\n  if (event.type === \"NEXT\") {\r\n    return {\r\n      ...state,\r\n      slideIndex:\r\n        state.slideIndex === 0 ? slides.length - 1 : state.slideIndex - 1\r\n    };\r\n  }\r\n};\r\n\r\nfunction Slide({ slide, offset }) {\r\n  const active = offset === 0 ? true : null;\r\n  const ref = useTilt(active);\r\n\r\n  return (\r\n    \r\n    <div\r\n      ref={ref}\r\n      className=\"slide\"\r\n      data-active={active}\r\n      style={{\r\n        \"--offset\": offset,\r\n        \"--dir\": offset === 0 ? 0 : offset > 0 ? 1 : -1\r\n      }}\r\n    >\r\n      <div\r\n        className=\"slideBackground\"\r\n      > <img className=\"slideBackground\" src={slide.image} alt={`Cocina estilo ${slide.title}, ${slide.description}`} rel=\"preload\" as=\"image\"/></div>\r\n\r\n      <div\r\n        className=\"slideContent\"\r\n        style={{\r\n          backgroundImage: `url('${slide.image}')`,\r\n          backgroundPosition: 'bottom',\r\n        }}\r\n      >\r\n        <div className=\"slideContentInner\">\r\n          <h2 className=\"slideTitle\">{slide.title}</h2>\r\n          <h3 className=\"slideSubtitle\">{slide.subtitle}</h3>\r\n          <p className=\"slideDescription\">{slide.description}</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nfunction Cocinas(props) {\r\n  var {displaySlides}=props;\r\n  let [displayed, setSlides] = useState(displaySlides)\r\n  const [state, dispatch] = useReducer(slidesReducer, initialState);\r\n  useEffect(() => {\r\n    setSlides(displayed=displaySlides)\r\n   state.slideIndex=0\r\n  }, [displaySlides])\r\n  if(displayed===\"LAMINADO\"){slides=laminado}\r\n  if(displayed===\"MELAMINA\"){slides=melamina}\r\n  if(displayed===\"CANTEADO\"){slides=canteado}\r\n  if(displayed===\"POLILAMINADO\"){slides=polilaminado}\r\n  if(displayed===\"LACADO\"){slides=lacado}\r\n  if(displayed===\"MADERA\"){slides=madera}\r\n\r\n  if (window.location.pathname==='/catalogo'){\r\n    window.onkeydown=(event)=>{\r\n      if(event.keyCode === 39){return dispatch({ type: \"PREV\" })}\r\n      else if(event.keyCode === 37){return dispatch({ type: \"NEXT\" })}\r\n      return null\r\n    }\r\n  } \r\n\r\n\r\n  return (\r\n   \r\n  \r\n    <div className=\"slides\">\r\n      <button onClick={() => dispatch({ type: \"NEXT\" })} style={{color:'black', marginLeft: '30vw'}} >‹</button>\r\n\r\n      {[...slides, ...slides, ...slides].map((slide, i) => {\r\n        let offset = slides.length + (state.slideIndex - i);\r\n        return <Slide slide={slide} offset={-offset} key={i} />;\r\n      })}\r\n      <button onClick={() => dispatch({ type: \"PREV\" })} style={{color:'black', marginRight:'30vw'}} >›</button>\r\n    </div>\r\n    \r\n  );\r\n}\r\n\r\nexport default Cocinas;","C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\NavBar.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\ContactForm.js",["165"],"import React, {useState} from 'react'\r\nimport emailjs from 'emailjs-com';\r\nimport '../styles/ContactForm.scss'\r\nrequire('dotenv').config();\r\n\r\n\r\n\r\nexport default function ContactForm() {\r\n\r\n  const [sendStatus,setFormStatus]=useState(false)\r\n  const [formInputs,setFormValues]=useState({name:'',email:'',phone:'',message:''})\r\n\r\n  const{\r\n    REACT_APP_EMAILJS_USERID: userId,\r\n    REACT_APP_EMAILJS_TEMPLATEID: templateId,\r\n    REACT_APP_EMAILJS_SERVICEID: serviceId,\r\n    REACT_APP_EMAILJS_RECEIVER: email,\r\n  }=process.env\r\n\r\n\r\n  let handleChange=(e)=>{\r\n    const{user_email,user_name,user_phone,message}=e.currentTarget;\r\n    setFormValues({name:user_name.value,email:user_email.value,phone:user_phone.value,message:message.value})\r\n    console.log(formInputs)\r\n  }\r\n\r\n  function validateEmail(mail) \r\n    {\r\n      if (/^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\\.[a-zA-Z0-9-]+)*$/.test(mail))\r\n        {\r\n          return (true)\r\n        }\r\n          alert(\"El formato del email no parece valido!\")\r\n          return (false)\r\n    }\r\n\r\n  function validateNotEmpty(input) \r\n    {\r\n      if (input.length>10)\r\n        {\r\n          return (true)\r\n        }\r\n          alert(\"Introduce un mensaje que explique tu motivo de contacto\")\r\n          console.log('here input', input.length)\r\n          return (false)\r\n    }\r\n\r\n  function validatePhone(phone) \r\n    {\r\n      if (/^[9|6|7]{1}([\\d]{2}[-]*){3}[\\d]{2}$/.test(phone) )\r\n        {\r\n          return (true)\r\n        }\r\n          alert(\"El formato del telefono no parece valido!\")\r\n          return (false)\r\n    }\r\n\r\n  function sendEmail(e) {\r\n    e.preventDefault();\r\n    if(\r\n      validateNotEmpty(formInputs['name'])&&\r\n      validateEmail(formInputs['email'])&&\r\n      validatePhone(formInputs['phone'])&&\r\n      validateNotEmpty(formInputs['message'])\r\n    ){\r\n      emailjs.sendForm(serviceId, templateId, e.target, userId)\r\n      .then((result) => {\r\n          console.log(result.text);\r\n      }, (error) => {\r\n          console.log(error.text);\r\n      });\r\n      setFormStatus(true);\r\n    }\r\n  }\r\n\r\n  return (\r\n    <form className=\"contact__form\" onSubmit={sendEmail} onChange={handleChange}>\r\n      <label>Nombre</label>\r\n      <input type=\"text\" name=\"user_name\" className=\"contact__form-input\"/>\r\n      <label>Email</label>\r\n      <input type=\"email\" name=\"user_email\" className=\"contact__form-input\"/>\r\n      <label>Teléfono</label>\r\n      <input type=\"text\" name=\"user_phone\" className=\"contact__form-input\"/>\r\n      <label>A tener en cuenta:</label>\r\n      <textarea name=\"message\" rows=\"6\" className=\"contact__form-text--area\"/>\r\n      {sendStatus? \r\n        <button type=\"submit\" value=\"Send\" className=\"contact__form-button--dissable\" disabled={true}> Enviado! </button>:\r\n        <button type=\"submit\" value=\"Send\" className=\"contact__form-button--enable\"> Enviar </button>\r\n      }\r\n    </form>\r\n  );\r\n}","C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Equipment.js",["166"],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\SectionBox.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Logo.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Error404.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Products.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Diseña_tu_cocina\\DiseñoMateriales.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Diseña_tu_cocina\\Laminados.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Diseña_tu_cocina\\SideBar.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Diseña_tu_cocina\\MainMenu.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Diseña_tu_cocina\\LaminadoLacado.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Diseña_tu_cocina\\Canteado.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Diseña_tu_cocina\\Polilaminado.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Diseña_tu_cocina\\Lacados.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Diseña_tu_cocina\\Maderas.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Diseña_tu_cocina\\Encimeras.js",["167","168","169","170","171","172","173"],"import React, {useState,useRef,useEffect} from 'react'\r\nimport '../../styles/Encimeras.scss'\r\nimport SideBar from './SideBar';\r\nimport {EncimerasData} from '../Diseña_tu_cocina/DesignData';\r\n\r\nexport default function Encimeras() {\r\n\r\n    const {estilos_formica, modelos_formica, estilos_cuarzo,modelos_cuarzo,types}=EncimerasData;\r\n    let [formicaToggle,setFormicaToggled]=useState(false);\r\n    let [cuarzoToggle,setCuarzoToggled]=useState(false);\r\n    const formica = useRef();\r\n    const cuarzo = useRef();\r\n    const executeScroll=()=>formicaToggle===true && formica.current.scrollIntoView()||cuarzoToggle===true && cuarzo.current.scrollIntoView()\r\n    useEffect(() => {\r\n        executeScroll()\r\n    }, [formicaToggle,cuarzoToggle])\r\n    // let isTouchScreen = (('ontouchstart' in window) ||(navigator.MaxTouchPoints > 0) ||(navigator.msMaxTouchPoints > 0));\r\n\r\n    const displayContent=(e)=>{\r\n        if(e.currentTarget.id=='formica'){setCuarzoToggled(false); setFormicaToggled(!formicaToggle)}\r\n        else if(e.currentTarget.id=='cuarzo'){setFormicaToggled(false); setCuarzoToggled(!cuarzoToggle)}\r\n        \r\n    }\r\n\r\n    let SectionOne=()=>{\r\n        return (\r\n                <section className=\"encimeras__section-one\" >\r\n                    <div className=\"encimeras__section-one--image-container\"></div>\r\n                    <div className=\"encimeras__section-one--header\" >\r\n                        <div className=\"encimeras__section-one--header-content\">\r\n                            <p className=\"encimeras__section-one--header--A\">ENCIMERAS</p>\r\n                        </div>\r\n                    </div>\r\n                </section>\r\n        )\r\n    }\r\n\r\n    let SectionTwo=()=>{\r\n        return (\r\n            <section className=\"encimeras__section-two\" >\r\n                <div >\r\n                    <div className=\"encimeras__section-two--header\">ENCIMERAS</div>\r\n                    <div className=\"encimeras__section-two--separation-bar\"></div>\r\n                    <br/><br/>\r\n                    <div className=\"encimeras__section-two--caracteristicas\">Las encimeras de cocina son, por así decirlo, la esencia de esta, ya que es el material que más espacio abarca y es una pieza clave e indispensable en la misma.\r\n                    <br/><br/>\r\n                    La cocina es un espacio muy personal donde sociabilizar, estar en familia, amigos, tener una reunión etc....por lo que la encimera es la primera imagen que atraerá la mirada de sus invitados.\r\n                    <br/><br/>\r\n                    A la hora de escoger una encimera para su cocina debería preguntarse<div style={{fontWeight:'900', display:'inline-block'}}>&nbsp;¿qué imagen desea transmitir?</div>\r\n                    <br/><br/>\r\n                    A continuación le dejamos una selección de los principales <div style={{fontWeight:'900', display:'inline-block'}}>grupos de materiales</div> que existen:\r\n                    <br/><br/>\r\n                    <div style={{fontWeight:'900'}}>- Madera</div>\r\n                    Formica o Laminadas y Madera maciza\r\n                    <br/>\r\n                    <div style={{fontWeight:'900'}}>- Piedras naturales</div>\r\n                    Cuarzo, granito, mármol\r\n                    <br/>\r\n                    <div style={{fontWeight:'900'}}>- Porcelánicos</div>\r\n                    Neolith, Dekton, Rak ceramics\r\n                    </div>\r\n                </div>\r\n            </section>\r\n        )\r\n    }\r\n\r\n    let SectionThree=()=>{\r\n        return (\r\n                <div className=\"encimeras__section-three\" >\r\n                    <div className=\"encimeras__section-three--image-container\" ></div>\r\n                    <div className=\"encimeras__section-three--content-container\" >\r\n                        <div className=\"encimeras__section-three--button-container\">\r\n                            <button onClick={displayContent} className=\"encimeras__section-three--button spin circle\" id=\"formica\" style={{outline:'none'}}> ENCIMERAS <br/> FORMICA </button>\r\n                            <button onClick={displayContent} className=\"encimeras__section-three--button spin circle\" id=\"cuarzo\" style={{outline:'none'}}> ENCIMERAS <br/> CUARZO</button>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n        )\r\n    }\r\n\r\n    let EncimerasFormica=()=>{\r\n        return (\r\n            <section ref={formica} className=\"encimeras__formica--info\">\r\n            <div style={{height:'fitContent'}}>\r\n                <div className=\"encimeras__formica--info-header\">Características Encimeras de Formica</div>\r\n                <div className=\"encimeras__formica--info-separation-bar\"></div>\r\n                <br/><br/>\r\n                <div className=\"encimeras__formica--info-caracteristicas\">Se trata de un proceso que conlleva el recubrimiento estratificado con resinas fenólicas sobre un material de soporte que suele ser DM o aglomerado, lo que permite imitar diferentes acabados. Es un material muy duro por lo que es difícil rayarlo y muy fácil de limpiar.\r\n                <br/><br/>  De la elección depende el resultado y para ello te ofrecemos todo tipo de materiales y acabados para configurar encimeras a medida, porque sabemos que en la variedad está el gusto y porque tenemos los mejores precios.\r\n                <br/><br/>Nosotros somos una apuesta segura.\r\n                </div>\r\n            </div>\r\n        </section>\r\n        )\r\n    }\r\n\r\n    let EncimerasCuarzo=()=>{\r\n        return (\r\n            <section ref={cuarzo} className=\"encimeras__cuarzo--info\">\r\n            <div style={{height:'fitContent'}}>\r\n                <div className=\"encimeras__cuarzo--info-header\">Características Encimeras de Cuarzo</div>\r\n                <div className=\"encimeras__cuarzo--info-separation-bar\"></div>\r\n                <br/><br/>\r\n                <div className=\"encimeras__cuarzo--info-caracteristicas\">Se trata de un material formado por un 94% de cuarzo, el otro 6% son resinas especiales y otros componentes que le aportan numerosas cualidades como soportar los impactos, la humedad, lo productos abrasivos y el contacto con el filo de objetos cortantes.\r\n                <br/><br/>  Siempre es recomendable poner un protector si se van a colocar sobre ella ollas o sartenes directamente del fuego.\r\n                <br/><br/> Es, mayoritariamente, de los materiales más usados para las encimeras de cocina, es un material muy competente gracias a su calidad precio.\r\n                </div>\r\n            </div>\r\n        </section>\r\n        )\r\n    }\r\n\r\n    let FormicaSection=()=>{\r\n            return(\r\n            <div className=\"encimeras__formica-cuarzo--grid-one\">\r\n\r\n                { modelos_formica.map((modelo,i)=>{\r\n                    return(\r\n                        <div key={`formica${i}`} className={ (i+1)%3===0? `encimeras__formica-cuarzo--one-unit-test ${types[i]}`:`encimeras__formica-cuarzo--one-unit ${types[i]}`}>\r\n                            <div className= {`encimeras__formica-cuarzo--one ${estilos_formica[i]}`}></div>\r\n                            <div className={`encimeras__formica-cuarzo--one-text `}>{modelo}</div>\r\n                        </div>\r\n                    )\r\n                })}\r\n                \r\n            </div>\r\n            )\r\n        }     \r\n\r\n    let FormicaColeccion=()=>{\r\n        return(\r\n            <div className=\"encimeras__formica-cuarzo--container-one\">\r\n                <div className=\"encimeras__formica-cuarzo--header\"> Colección </div>\r\n                <FormicaSection/>\r\n            </div>\r\n        )\r\n    }\r\n\r\n    let CuarzoSection=()=>{\r\n        return(\r\n        <div className=\"encimeras__formica-cuarzo--grid-one dark\">\r\n\r\n            { modelos_cuarzo.map((modelo,i)=>{\r\n                return(\r\n                    <div key={`cuarzo${i}`} className={`encimeras__formica-cuarzo--one-unit ${types[i]}`}>\r\n                        <div className= {`encimeras__formica-cuarzo--one ${estilos_cuarzo[i]}`}></div>\r\n                        <div className={`encimeras__formica-cuarzo--one-text `}>{modelo}</div>\r\n                    </div>\r\n                )\r\n            })}\r\n            \r\n        </div>\r\n        )\r\n    }     \r\n\r\n\r\nlet CuarzoColeccion=()=>{\r\n    return(\r\n        <div className=\"encimeras__formica-cuarzo--container-one\">\r\n            <div className=\"encimeras__formica-cuarzo--header\"> Colección </div>\r\n            <CuarzoSection/>\r\n        </div>\r\n    )\r\n}\r\n    \r\n\r\n    \r\n\r\n return(\r\n     <div id=\"encimeras\" style={{overflow:'hidden'}}>\r\n        <SideBar/>\r\n        <SectionOne/>\r\n        <SectionTwo/>\r\n        <SectionThree/>\r\n        {formicaToggle && <EncimerasFormica/>}\r\n        {formicaToggle && <FormicaColeccion/>}    \r\n        {cuarzoToggle && <EncimerasCuarzo/>}\r\n        {cuarzoToggle && <CuarzoColeccion/>}\r\n     </div>\r\n    \r\n )\r\n}\r\n","C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Equipamientos\\Bisagras.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Equipamientos\\Cajones.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Equipamientos\\Colgadores.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Equipamientos\\Tiradores.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Equipamientos\\Patas.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\Equipamientos\\Elevadores.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\CocinaSlideData.js",[],"C:\\Users\\Gunner\\Desktop\\Proyecto Andrea\\final\\cocinas\\src\\components\\TrabajosRealizadosUnit.js",[],"C:\\Users\\Gun